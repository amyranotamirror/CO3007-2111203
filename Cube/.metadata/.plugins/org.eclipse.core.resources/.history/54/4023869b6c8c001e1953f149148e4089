#ifndef INC_BUTTON_H_
#define INC_BUTTON_H_

#include "global.h"
enum BUTTON_STATE {IS_RELEASE,
				   IS_PRESSED,
				   IS_PRESSED_DELAY,
				   IS_LONG_PRESSED};

struct Button;
void initButton();
void readButton();
uint16_t isButtonPressed(uint16_t index);


#endif /* INC_BUTTON_H_ */
